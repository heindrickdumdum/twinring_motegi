        <!-- restaurant menu -->
        <section class="restaurant-menu">
            <div class="l-section-title">
              <%- include('/partials/heading', {
                modifier: 'heading-wrap-center heading-restaurant-menu',
                text: 'Menu',
                subtextvg: 'oak-subtext',
                altSubtextvg: 'メニュー'
              }) %>
            </div>
            
            <!-- gelato -->
            <%- include('/partials/restaurant/section-menu-gelato', {
              data: getData().data.roccoGelatoMenu,
              modifier: '',
              icon: mix('/assets/images/rocco/gelato-img.png'),
              iconalt: 'ジェラート',
              modifierjp: 'heading-restaurant-menu-gelato-jp',
              svgjp: 'gelato-subtext',
              svgjpalt: 'ジェラート',
              modifieren: 'heading-restaurant-menu-gelato-en',
              svgen: 'gelato-eng-subtext',
              svgenalt: 'Gelato'
            }) -%>
            
            <!-- gelato pop -->
            <%- include('/partials/restaurant/section-menu-gelato', {
              data: getData().data.roccoGelatoPopMenu,
              icon: mix('/assets/images/rocco/gelatopop-img.png'),
              iconalt: 'ジェラートポップ',
              modifierjp: 'heading-restaurant-menu-gelato-jp heading-gelatopop-jp',
              svgjp: 'gelatopop-subtext',
              svgjpalt: 'ジェラートポップ',
              modifieren: 'heading-restaurant-menu-gelato-en heading-gelatopop-en',
              svgen: 'gelatopop-eng-subtext',
              svgenalt: 'Gelato Pop'
            }) -%>

        </section>

        <!-- restaurant banner -->
        <div class="restaurant-banner">
          <div class="subpage-inner">
            <div class="banner-text">
              <h2 class="banner-title">
                <span>フレーバーの変更・売り切れについて</span>
              </h2>
              <p class="banner-desc">
                ※季節によりご用意するフレーバーが異なりますので店舗のショーケースよりお選びください。<br/>
                ※ジェラートのフレーバーは予告なしに変わることがございます。 また、売り切れになる場合もございますので予めご了承ください。
              </p>
            </div>
          </div>
        </div>

        <!-- restaurant sub menu rocco -->
        <div class="restaurant-sub-menu">
          <div class="subpage-inner">
            <div class="sub-menu-container">
              <ul class="sub-menu-list">
                <% getData().data.roccoSubMenu.forEach(item => { %>
                  <li class="sub-menu-item">
                    <div class="menu-item-container">
                      <div class="sub-menu-image u-show-pc">
                        <img src="<%- item.img -%>" alt="<%- item.title -%>">
                      </div>
                      <div class="sub-menu-title">
                        <span class="main-text">
                          <%- item.title -%>
                        </span>
                        <span class="sub-text">
                          <%- item.subTitle -%>
                        </span>
                      </div>
                      <div class="sub-menu-price">
                        <span>
                          <%- item.price -%>
                        </span>
                      </div>
                    </div>
                  </li>
                <% }); %>
              </ul>
            </div>
          </div>
        </div>

        <!-- restaurant rocco drink menu -->
        <div class="restaurant-drink-menu">
          <div class="subpage-inner">

            <div class="l-section-title">
              <%- include('/partials/heading', {
                modifier: 'heading-wrap-center heading-restaurant-menu-gelato-jp heading-menu-drink-jp',
                subtextvg: 'rocco-drinks-text-jp',
                altSubtextvg: 'ドリンク、その他'
              }) %>
            </div>
        
            <div class="l-section-title dotted-bg">
              <%- include('/partials/heading', {
                modifier: 'heading-wrap-center heading-restaurant-menu-gelato-en heading-menu-drink-en',
                subtextvg: 'rocco-drinks-text-en',
                altSubtextvg: 'Drink & Others'
              }) %>
            </div>

            <div class="menu-drink-container">
              <div class="menu-drink-box">
                <ul class="menu-drink-list">
                  <% getData().data.roccoDrinksMenu1.forEach(item => { %>
                    <li class="drink-menu-item">
                      <div class="drink-menu-title">
                        <span><%- item.title -%></span>
                        <span><%- item.price -%></span>
                      </div>
                      <% if( item.subtext !== '' ) { %>
                      <div class="drink-menu-subtext">
                        <span><%- item.subtext -%></span>
                      </div>
                      <% } %>
                      <div class="drink-menu-desc">
                        <p>
                          <%- item.desc -%>
                        </p>
                      </div>
                    </li>
                  <% }); %>
                </ul>
              </div>

              <div class="menu-drink-box">
                <ul class="menu-drink-list">
                  <% getData().data.roccoDrinksMenu2.forEach(item => { %>
                    <li class="drink-menu-item drink-menu2-item">
                      <div class="drink-menu-title drink-menu2-title">
                        <span><%- item.title -%></span>
                        <span><%- item.price -%></span>
                      </div>
                      <% if( item.subtext !== '' ) { %>
                      <div class="drink-menu-subtext drink-menu2-subtext">
                        <span><%- item.subtext -%></span>
                      </div>
                      <% } %>
                    </li>
                  <% }); %>
