            <div class="l-section-title u-show-sp battle-cart-section-title">
              <%- include('/partials/heading', {
                modifier: 'heading-wrap-center heading-mobipark heading-racing',
                text: 'Battle Cart',
                alt: 'Battle Cart',
                altSubtext: '',
                subtextvg: 'battle-cart-sp'
              }) %>
            </div>
          </div>
          <section class="subpage-section subpage-section-racing subapge-section-goods-tab">
            <%- include('/partials/racingkart/racing-tabs' ) -%>
          </section>
        </div>
      </div>

      <div class="field-introduction battle-cart-layout">
        <div class="field-introduction-container">
          <div class="u-show-pc">
            <%- include('/partials/heading', {
              modifier: 'heading-wrap-how-to heading-wrap-center heading-wrap-hellowoods',
              text: 'Course Layout',
              alt: '',
              subtextvg: 'course-layout-svg',
              altSubtext: ''
            }) %>
          </div>
          <div class="u-show-sp">
            <%- include('/partials/heading', {
              modifier: 'heading-wrap-how-to heading-wrap-center heading-wrap-hellowoods',
              text: 'Course Layout',
              alt: '',
              subtextvg: 'course-layout-svg-sp',
              altSubtext: ''
            }) %>
          </div>

          <div class="map-wrapper">
            <img class="map-image u-show-pc" src="<%= mix('/assets/images/racingkart/map.png') %>" alt="course layout">
            <img class="map-image u-show-sp" src="<%= mix('/assets/images/racingkart/map-sp.png') %>" alt="course layout">
          </div>
          <div class="smap">
            <div class="l-container-racing">
              <img class="map-flag" src="<%= mix('/assets/images/racingkart/map-flag.jpg') %>" alt="map flag">
            </div>
          </div>
        </div>
      </div>

      <section class="schedule-section" id="schedule">
        <div class="l-container-racing">
          <div class="l-section-title">
            <%- include('/partials/heading', {
              modifier: 'heading-wrap-center heading-wrap-business-hours-guide',
              text: 'Schedule',
              alt: 'Schedule',
              altSubtext: '',
              subtextvg: 'schedule'
            }) %>
          </div>
          <div class="l-container-m business-calendar-wrapper">
            <div class="business-calendar-inner">
              <div class="responsive-calendar u-show-pc">
                <iframe src="//www.google.com/calendar/embed?showTitle=0&amp;?height=600&amp;wkst=2&amp;bgcolor=%23FFFFFF&amp;src=53u3shra2nqa1buep9ain7sis4%40group.calendar.google.com&amp;color=%23B1365F&amp;ctz=Asia%2FTokyo" style=" border-width:0 " width="960" height="600" frameborder="0" scrolling="no"></iframe>
              </div>
              <div class="responsive-calendar u-show-sp">
                <iframe src="//www.google.com/calendar/embed?showTitle=0&amp;mode=AGENDA&amp;height=250&amp;wkst=2&amp;bgcolor=%23FFFFFF&amp;src=53u3shra2nqa1buep9ain7sis4%40group.calendar.google.com&amp;color=%23B1365F&amp;ctz=Asia%2FTokyo&amp;showTabs=0&amp;showPrint=0" style="" width="94%" height="320" frameborder="0" scrolling="no"></iframe>
              </div>
              <p class="calendar-note">※イベント開催や団体様のご利用により、コースが貸切となる場合がございます。</p>
            </div>
          </div>
        </div>
      </section>

      <section class="member-pass-section">
