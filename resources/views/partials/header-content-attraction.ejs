<div class="header-sub-content-inner js-header-sub-content-inner" id="header-sub-attraction">

  <a class="header-sub-top-button" href="#">
    <i class="header-sub-top-button-icon">
      <%- include('/partials/svg', {
        name: 'activity'
      }) %>
    </i>
    アトラクション・アクティビティTOP
    <i class="header-sub-top-button-arrow">
      <%- include('/partials/svg', {
        name: 'arrow'
      }) %>
    </i>
  </a>

  <div class="header-sub-inner">
    <div class="header-sub-inner-block">
      <div class="header-sub-image-list-wrap">
        <% getData().data.headerAttractionImageList.forEach( list => { %>
          <ul class="header-sub-image-list">
            <% list.forEach( item => { %>
            <li class="header-sub-image-item">
              <a class="header-sub-image-link" href="#">
                <div class="header-sub-image<% if(list.length > 3) { %> header-sub-image-small<% } %>" style="background-image: url(<%- item.image %>);"></div>
                <span class="header-sub-image-text"><%- item.text %></span>
              </a>
            </li>
            <% }) %>
          </ul>
        <% }) %>
      </div>
    </div>
    <div class="header-sub-inner-block header-sub-inner-block-right">
      <ul class="header-sub-button-list">
        <% getData().data.headerAttractionRightLinks.forEach( item => { %>
        <li class="header-sub-button-item">
          <a class="header-sub-button-link" href="<%- item.link %>">
            <%- item.text %>
            <i class="header-sub-button-link-icon">
              <%- include('/partials/svg', {
                name: 'arrow'
              }) %>
            </i>
          </a>
        </li>
        <% }) %>
      </ul>
    </div>
  </div>
  <div class="header-sub-bottom">
    <ul class="header-sub-bottom-list">
      <% getData().data.headerAttractionBottomLinks.forEach( item => { %>
      <li>
        <a href="<%- item.link %>"><%- item.text %></a>
      </li>
      <% }) %>
    </ul>
  </div>

</div>
