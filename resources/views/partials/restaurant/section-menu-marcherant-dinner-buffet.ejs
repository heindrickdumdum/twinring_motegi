<section class="restaurant-menu marcherant-menu">
  <div class="l-container">
    <div class="l-section-title">
      <%- include('/partials/heading', {
        modifier: 'heading-wrap-auto',
        heading_level: 1,
        text: 'Other Menu',
        subtextvg: 'marcherant-dinner-buffet-other-menu',
        altSubtextvg: 'その他のレストラン'
      }) %>
    </div>
    <ul class="menu-container">
      <% data.forEach(item => { %>
        <li class="menu-item">
          <a class="menu-link" href="<%- item.link -%>">
            <div class="menu-image" style="background-image:url(<%= item.image %>);"></div>
            
            <p class="menu-desc menu-desc-marcherant"><%- item.desc %>
              <i class="button-gradient-icon">
                <%- include('/partials/svg', {
                  name: 'arrow'
                }) %>
              </i>
            </p>
          </a>
        </li>
        </li>
      <% }) %>
    </ul>
  </div>
</section>